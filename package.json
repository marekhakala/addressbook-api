{
  "name": "address-book-api-application",
  "version": "1.0.0",
  "description": "Address book is an address book web API application for SPA and mobile applications.",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production babel-node --presets env,stage-2 ./server.js",
    "dev": "NODE_ENV=development nodemon ./server.js --exec babel-node --presets env,stage-2",
    "seed": "babel-node --presets env,stage-2 ./seed.js",
    "test": "NODE_ENV=test mocha --timeout 10000 --compilers js:babel-register"
  },
  "author": "Marek Hakala <hakala.marek@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-cli": "6.26.0",
    "body-parser": "1.16.0",
    "cors": "2.8.2",
    "dotenv": "3.0.0",
    "ejs": "2.5.6",
    "errorhandler": "1.5.0",
    "express": "4.15.0",
    "express-jwt": "5.1.0",
    "express-session": "1.15.0",
    "firebase": "4.5.0",
    "firebase-admin": "5.4.1",
    "jsonwebtoken": "7.3.0",
    "method-override": "2.3.7",
    "methods": "1.1.2",
    "mongoose": "4.8.2",
    "mongoose-unique-validator": "1.0.4",
    "morgan": "1.8.0",
    "nodemon": "1.11.0",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "request": "2.80.0",
    "swagger-jsdoc": "1.9.7",
    "swagger-ui-express": "2.0.8",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "3.5.0",
    "chai-http": "2.0.1",
    "mocha": "2.4.5"
  }
}
